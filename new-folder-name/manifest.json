{
  "manifest_version": 3,
  "name": "X.com Redirector",
  "version": "1.0",
  "description": "Automatically redirects any x.com links to xcancel.com.",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["*://x.com/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
